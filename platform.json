{
  "name": "atmelavr-current",
  "title": "Atmel AVR with up to date toolchain builds",
  "description": "Supports Atmel AVR 8- and 32-bit MCUs. Fork of atmelavr platform, featuring latest toolchain builds. https://github.com/haarer/toolchain68k",
  "url": "http://www.atmel.com/products/microcontrollers/avr/default.aspx",
  "homepage": "https://github.com/haarer/platform-atmelavr",
  "license": "Apache-2.0",
  "keywords": [
      "dev-platform",
      "Microchip",
      "AVR",
      "Atmel",
      "8-bit"
  ],
  "engines": {
    "platformio": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/haarer/platform-atmelavr.git"
  },
  "version": "2.2.0",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json",
    "https://raw.githubusercontent.com/eerimoq/simba/master/make/platformio/manifest.json",
    "https://github.com/haarer/toolchain68k/releases/download/gcc920/manifest.json"
  ],
  "frameworks": {
    "arduino": {
      "package": "framework-arduino-avr",
      "script": "builder/frameworks/arduino.py"
    },
    "simba": {
      "package": "framework-simba",
      "script": "builder/frameworks/simba.py"
    }
  },
  "packages": {
    "toolchain-avr-current": {
      "type": "toolchain",
      "owner": "platformio",
      "version": ">=8.2.0"
    },
    "framework-arduino-avr": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~5.0.0"
    },
    "framework-arduino-avr-attiny": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~1.3.2"
    },
    "framework-arduino-avr-bean": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~2.0.3"
    },
    "framework-arduino-avr-core13": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~0.0.22"
    },
    "framework-arduino-avr-digistump": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~1.6.7"
    },
    "framework-arduino-avr-dwenguino": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~2.2.1"
    },
    "framework-arduino-avr-megacore": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~2.0.3"
    },
    "framework-arduino-avr-mightycore": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~2.0.5"
    },
    "framework-arduino-avr-minicore": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~2.0.5"
    },
    "framework-arduino-avr-majorcore": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~2.0.2"
    },
    "framework-arduino-avr-microcore": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~1.0.6"
    },
    "framework-arduino-avr-nicai": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~3.5.0"
    },
    "framework-arduino-avr-panstamp": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~1.5.7"
    },
    "framework-arduino-avr-prusa_rambo": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": "~1.0.3"
    },
    "framework-simba": {
      "type": "framework",
      "optional": true,
      "owner": "platformio",
      "version": ">=7.0.0"
    },
    "tool-avrdude": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~1.60300.0"
    },
    "tool-micronucleus": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~1.200.0"
    },
    "tool-simavr": {
      "type": "debugger",
      "optional": true,
      "owner": "platformio",
      "version": "~1.10600.200511"
    }
  }
}
